#%RAML 1.0 DataType

type: object
properties:
  flightNumber: number
  startDate: string
  origin: string
  destination: string
  noOfPassengers: integer
  isReturn: boolean
  nameOfPassenger:
    type: object
    required: true
    properties:
      fname: string
      lname: string
  bookingType: string
  insuranceRequired: boolean
  username: string
  isGuest: boolean
  couponName: string
  contactDetails: number
  